<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>

    <PackageId>Ab4d.SharpEngine.Logging.Serilog</PackageId>
    <PackageVersion>1.0.1</PackageVersion>
    <Authors>Austin Powder Company</Authors>
    <Description>Utilities for writing Ab4d.SharpEngine logs to Serilog loggers</Description>
    <Copyright>Copyright (c) Austin Powder Company 2024</Copyright>
    <PackageProjectUrl>https://github.com/Austin-Powder-Company/Ab4d.SharpEngine.Logging.Serilog</PackageProjectUrl>
    <PackageIcon>icon.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageTags>Ab4d;SharpEngine;Log;Serilog</PackageTags>
    <PackageLicenseExpression>0BSD</PackageLicenseExpression>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Ab4d.SharpEngine" Version="2.0.8956" />
    <PackageReference Include="Serilog" Version="4.0.2" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\sharpengine-contrib\icon.png" Pack="true" PackagePath="\"/>
    <None Include="..\README.md" Pack="true" PackagePath="\"/>
  </ItemGroup>
  
</Project>
